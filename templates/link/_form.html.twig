{{ form_start(form) }}
    {{ form_widget(form) }}

    {% if isUpdate %}
        <button class="btn btn-primary w-100"><i class="fa fa-edit" aria-hidden="true"></i>  {{ button_label|default('Modifier le lien') }}</button> 
    {% else %}
        <button class="btn btn-primary w-100"><i class="fa fa-plus" aria-hidden="true"></i>  {{ button_label|default('Ajouter le lien') }}</button> 
    {% endif %}
    
{{ form_end(form) }}
